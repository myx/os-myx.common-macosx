#!/bin/sh

##### !!! THIS SCRIPT MUST BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

GetCpuCount(){
	sysctl -n hw.ncpu
	# sysctl -n kern.smp.cpus
}


case "$0" in
	*/myx.common/bin/os/getCpuCount) 
		if [ "$1" = "--help" ] ; then
			echo "Syntax: myx.common os/getCpuCount" >&2
			exit 1
		fi
		set -e
		GetCpuCount "$@"
	;;
esac
