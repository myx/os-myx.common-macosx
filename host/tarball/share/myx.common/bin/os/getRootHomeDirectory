#!/bin/sh

##### !!! THIS SCRIPT MUST BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

OsGetRootHomeDirectory(){
	echo "/var/root"
}


case "$0" in
	*/myx.common/bin/os/getRootHomeDirectory) 
		if [ "$1" = "--help" ] ; then
			echo "Syntax: myx.common os/getRootHomeDirectory" >&2
			exit 1
		fi
		# set -e
		# OsGetRootHomeDirectory "$@"
		echo "/var/root"
	;;
esac
