#!/bin/sh

##### !!! THIS SCRIPT MUST BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

GetCommonScreenRc(){
	echo /usr/local/etc/screenrc
}


case "$0" in
	*/myx.common/bin/os/getCommonScreenRc) 
		if [ "$1" = "--help" ] ; then
			echo "Syntax: myx.common os/getCommonScreenRc" >&2
			exit 1
		fi
		# set -e
		# GetCommonScreenRc "$@"
		echo /usr/local/etc/screenrc
	;;
esac
