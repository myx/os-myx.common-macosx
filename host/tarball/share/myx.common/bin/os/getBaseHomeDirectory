#!/bin/sh

##### !!! THIS SCRIPT MUST BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

GetBaseHomeDirectory(){
	echo "/Users"
}


case "$0" in
	*/myx.common/bin/os/getBaseHomeDirectory) 
		if [ "$1" = "--help" ] ; then
			echo "Syntax: myx.common os/getBaseHomeDirectory" >&2
			exit 1
		fi
		# set -e
		# GetBaseHomeDirectory "$@"
		echo "/Users"
	;;
esac
